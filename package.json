{
  "name": "github-trending",
  "version": "1.0.0",
  "description": "A web app to list most starred Github repos that were created in the last 30 days",
  "main": "App.js",
  "scripts": {
    "dev": "npm run build:css && parcel public/index.html",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css"
  },
  "author": "Ravi Varshney",
  "license": "ISC",
  "dependencies": {
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "standard": "^14.3.4",
    "tailwindcss": "^1.6.2"
  }
}
